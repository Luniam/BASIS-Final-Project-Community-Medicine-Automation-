@using Microsoft.AspNet.Identity

@{
    ViewBag.Title = "Index";
}

<h2>Center : @User.Identity.GetUserName() </h2>

<div class="row">
    <div class="col-md-6">
        <h2>Doctor Entry</h2>
        <p class="btn btn-default">
            @Html.ActionLink("Entry", "CreateDoctor", "Center")
        </p>
    </div>
    
    <div class="col-md-6">
        <h2>See medicine stock report</h2>
        <p class="btn btn-default">
            @Html.ActionLink("Report", "SeeMedReport", "Center" )
        </p>
    </div>
    
    
    <div class="col-md-6">
        <h2>Give Treatment</h2>
        <p class="btn btn-default">
            @Html.ActionLink("Treatment", "GiveTreatMent", "Center")
        </p>
    </div>

</div>

